{
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "setupFilesAfterEnv": [
    "<rootDir>/src/setupTests.js"
  ],
  "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/frontend",
      "src/shared"
    ]
}